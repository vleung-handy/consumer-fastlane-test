<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
    >

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/pro_team_pro_list_swipe_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <com.handybook.handybook.ui.view.EmptiableRecyclerView
            android:id="@+id/pro_team_pro_list_recycler_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:listitem="@layout/layout_pro_team_pro_card"

            />
    </android.support.v4.widget.SwipeRefreshLayout>

    <include layout="@layout/layout_pro_team_empty"/>

</FrameLayout>
