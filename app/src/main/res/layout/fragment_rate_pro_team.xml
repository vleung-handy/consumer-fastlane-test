<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:id="@+id/rate_dialog_pro_match_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="@dimen/default_padding"
    >

    <include layout="@layout/rate_pro_team_item"
             android:id="@+id/simple_container"
        />

    <com.handybook.handybook.booking.ui.view.SwipeableViewPager
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:swipeable="false"
        app:wrap_content_enabled="true"
        />

</LinearLayout>
